<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >

<hibernate-mapping>
	<class name="fr.umlv.smoreau.beontime.model.timetable.Course" table="cours">
		<id
			column="id_cours"
			name="IdCourse"
			type="java.lang.Long"
		>
			<generator class="vm" />
		</id>
		<property
			column="id_formation"
			length="8"
			name="IdFormation"
			not-null="true"
			type="java.lang.Long"
		 />
		<property
			column="nb_semaine"
			length="2"
			name="NbWeeks"
			not-null="false"
			type="integer"
		 />
		<property
			column="date_fin"
			name="EndDate"
			not-null="false"
			type="calendar"
		 />
		<property
			column="date_debut"
			name="BeginDate"
			not-null="false"
			type="calendar"
		 />

		<many-to-one
			class="fr.umlv.smoreau.beontime.model.timetable.CourseType"
			name="IdCourseType"
			not-null="true"
		>
			<column name="id_type_cours" />
		</many-to-one>

		<set inverse="true" name="GroupsSubjectsTakingPart" lazy="true">
			<key column="id_cours" />
			<one-to-many class="fr.umlv.smoreau.beontime.model.association.TakePartGroupSubjectCourse" />
		</set>
		
		<set inverse="true" name="TeachersDirecting" lazy="true">
			<key column="id_cours" />
			<one-to-many class="fr.umlv.smoreau.beontime.model.association.IsDirectedByCourseTeacher" />
		</set>

		<set
			cascade="none"
			name="Materials"
			table="utilise_cours_materiel"
			lazy="true"
		>
			<key column="id_cours" />
			<many-to-many class="fr.umlv.smoreau.beontime.model.element.Material" column="id_materiel" />
		</set>
		<set
			cascade="none"
			name="Rooms"
			table="a_lieu_cours_local"
			lazy="true"
		>
			<key column="id_cours" />
			<many-to-many class="fr.umlv.smoreau.beontime.model.element.Room" column="id_local" />
		</set>
	</class>
</hibernate-mapping>
