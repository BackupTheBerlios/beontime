<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >

<hibernate-mapping>
	<class name="fr.umlv.smoreau.beontime.model.Group" table="groupe">
		<id
			column="id_groupe"
			name="IdGroupe"
			type="java.lang.Long"
		>
			<generator class="vm" />
		</id>
		<property
			column="id_formation"
			name="IdFormation"
			type="java.lang.Long"
		 />
		<property
			column="Intitule"
			length="20"
			name="Intitule"
			not-null="false"
			type="string"
		 />

		<set inverse="true" name="ParticipeGroupeMatiereCoursSet">
			<key column="id_groupe" />
			<one-to-many class="fr.umlv.smoreau.beontime.model.association.ParticipeGroupSubjectCourse" />
		</set>
		<joined-subclass name="fr.umlv.smoreau.beontime.model.association.AppartientEtudiantGroupe" table="Appartient_Etudiant_Groupe">
			<key column="id_groupe" />
			<property
				column="id_etudiant"
				length="8"
				name="IdEtudiant"
				not-null="true"
				type="java.lang.Long"
			 />
		</joined-subclass>
	</class>
</hibernate-mapping>
